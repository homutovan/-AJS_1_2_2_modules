{
  "extends": "airbnb-base",
  "env": {
     "es6": true,
     "browser": true,
     "jest": true
  },
  "rules": {
    "no-restricted-syntax": [
      "error",
      "LabeledStatement",
      "WithStatement"
    ],
    "max-classes-per-file": ["error", 2],
    "no-unused-vars": 0,
    "class-methods-use-this": ["error", { "exceptMethods": ["start"] }],
    "no-console": ["error", { "allow": ["warn", "error", "log"] }]
  }
}